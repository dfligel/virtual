# This file describes the network interfaces available on your system
# and how to activate them. For more information, see interfaces(5).

source /etc/network/interfaces.d/*

# The loopback network interface
auto lo
iface lo inet loopback

auto ens0
iface ens0 inet loopback

auto ens1
iface ens1 inet loopback

auto ens2
iface ens2 inet loopback

auto ens3
iface ens3 inet loopback

auto ens4
iface ens4 inet loopback

auto ens5
iface ens5 inet loopback

auto ens6
iface ens6 inet loopback

auto br-ex
iface br-ex inet dhcp

auto mgmt
iface mgmt inet static
address 192.168.99.3
netmask 255.255.255.0

